<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="gamefield_world">

    <!-- Default light -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Default ground -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Your gamefield model -->
    <include>
      <uri>model://gamefield</uri>
      <name>gamefield</name>
      <pose>0 0 0 0 0 0</pose>
    </include>

  </world>
</sdf>
